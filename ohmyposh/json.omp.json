{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#F1FA8C",
          "properties": {
            "style": "folder"
          },
          "style": "diamond",
          "template": "<b>{{ .Path }}</b>",
          "type": "path"
        },
        {
          "foreground": "#43CCEA",
          "properties": {
            "branch_icon": ""
          },
          "style": "diamond",
          "template": "<#ffffff> \u26A1 </><b>{{ .HEAD }}</b>",
          "type": "git"
        }
        
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
            "foreground": "#FFFFFF",
            "style": "plain",
            "template": "<#81bbef>></><#c9e2f8>></>",
            "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
